{
  "name": "neural-activity-visualizer-react",
  "version": "1.1.0",
  "description": "A ReactJS component for visualizing neural activity data",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "type": "module",
  "scripts": {
    "build-lib": "vite build",
    "build-lib-watch": "vite build --watch",
    "build-demo": "npm run build-lib && cd demo && npm run build",
    "start-demo": "cd demo && npm run start",
    "i-all": "npm i && cd demo && npm i",
    "ci-all": "npm ci && cd demo && npm ci",
    "dev": "npm-run-all --parallel build-lib-watch start-demo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "Andrew Davison <andrew.davison@cnrs.fr>, CNRS",
  "license": "MIT",
  "homepage": "https://github.com/NeuralEnsemble/neo-viewer/blob/master/js/react#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeuralEnsemble/neo-viewer.git",
    "directory": "js/react"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.3",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "plotly.js": "^2.35.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-plotly.js": "^2.6.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": "^6",
    "@mui/material": "^6",
    "plotly.js": "^2",
    "react": "^19",
    "react-dom": "^19",
    "react-plotly.js": "^2.6"
  }
}
